  {
    "Heex": {
      "prefix": "heex",
      "body": "~H\"\"\"\n$0\n\"\"\""
    },
    "Component": {
      "prefix": "cmp",
      "body": "def $1(assigns) do \n~H\"\"\"\n$0\n\"\"\"\n end"
    },
    "Placeholders": {
      "prefix": "pl",
      "body": "T1=${1:aa}\nT2=${2:<$1>}"
    },
    "Choices": {
      "prefix": "ch",
      "body": "T1=${1|a,b|} T2=${2|c,d|}"
    },
    "Linked": {
      "prefix": "li",
      "body": "T1=$1\n\tT1=$1"
    },
    "Variables": {
      "prefix": "va",
      "body": "Runtime: $VIMRUNTIME\n"
    },
    "Complex": {
      "prefix": "co",
      "body": [
        "T1=${1:$RANDOM}",
        "T3=${3:$1_${2:$1}}",
        "T2=$2"
      ]
    }
  }
